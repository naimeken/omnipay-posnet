{
  "name": "naimeken/omnipay-posnet",
  "type": "library",
  "description": "Posnet(Yapıkredi, Vakıfbank, Anadolubank sanal pos) gateway for Omnipay payment processing library",
  "homepage": "https://git.alegradigital.com/microservices/payment-posnet",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Omnipay\\PosNet\\": "src/"
    }
  },
  "require": {
    "php": "^7.3 || ^8.0",
    "omnipay/common": "^3",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-dom": "*"
  },
  "autoload-dev": {
    "psr-4": {
      "OmnipayTest\\PosNet\\": "tests/"
    }
  },
  "require-dev": {
    "omnipay/tests": "^3",
    "squizlabs/php_codesniffer": "^3",
    "phpro/grumphp": "^0.18"
  }
  ,
  "prefer-stable": true
}
